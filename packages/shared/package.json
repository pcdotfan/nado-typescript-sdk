{
  "name": "@nadohq/shared",
  "version": "0.1.0-alpha.26",
  "type": "module",
  "sideEffects": false,
  "description": "Shared utilities, types, and contract helpers for Nado SDK",
  "author": "Frank Jia <frank@inkfnd.com>",
  "homepage": "",
  "license": "ISC",
  "source": "./src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/nadohq/nado-typescript-sdk",
  "scripts": {
    "clean": "rm -rf dist && rm -f tsconfig.tsbuildinfo",
    "build": "tsup",
    "dev": "tsc -w",
    "lint": "eslint --cache './**/*.{ts,tsx}'",
    "lint:fix": "eslint --cache --fix './**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "peerDependencies": {
    "bignumber.js": "^9.3.0",
    "viem": "*"
  },
  "devDependencies": {
    "@jest/globals": "*",
    "bignumber.js": "^9.3.0",
    "viem": "*"
  },
  "gitHead": "494d15823821c58c8bd629e5945a4aa7a85a29f0"
}
